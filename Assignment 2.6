                for(int i=a;i<=b;i++)
                {
                  if(i%2==0)
                  {
                    System.out.println(i+" ");
                  }
                }
                System.out.println("The odd numbers between "+a+" and "+b+"are");
                for(int i=a;i<=b;i++)
                {
                  if(i%2!=0)
                  {
                    System.out.println(i+" ");
                  }
                }
                }
              }

